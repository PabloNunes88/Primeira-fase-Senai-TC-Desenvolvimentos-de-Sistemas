
/*Verificando Votação: Peça ao usuário para inserir sua idade e verifique 
se ele é elegível para votar (idade mínima de 16 anos).*/

programa {
  funcao inicio() {

    inteiro idade, somaTotal

    escreva("Insira sua idade: ")
    leia(idade)
    se(idade>=16){
      escreva("Você já pode votar: ")
    }senao{
      escreva("Você não pode votar ainda: ")
    }





  }
}
